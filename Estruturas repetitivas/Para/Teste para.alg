Algoritmo "teste_para"

Var

   N, i, x, soma : inteiro


Inicio

   escreva("Quantos números serão digitados: ")
   leia(n)
   
   soma <- 0

    para i de 1 ate N faca
    
       escreva("Digite um número: ")
       leia(x)
       soma <- soma + x
    
    fimpara

    escreval("Soma= ", soma)
    
    
Fimalgoritmo