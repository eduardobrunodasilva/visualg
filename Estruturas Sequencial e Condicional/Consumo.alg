Algoritmo "consumo"


Var

   Distancia: inteiro
   Combustivel, Consumo: real
   


Inicio

   escreva("Digite a distancia percorrida: ")
   leia(distancia)
   escreva("Digite a quantidade de combustivel: ")
   leia(combustivel)
   
   Consumo <- distancia/combustivel
   
   escreval("Consumo médio = ", consumo:8:3)


Fimalgoritmo